//go:build wireinject
// +build wireinject

package main

import (
	"{{ .ModuleName }}/internal/config"
	"{{ .ModuleName }}/internal/handler"
	"{{ .ModuleName }}/internal/router"
	"{{ .ModuleName }}/internal/server"
	"github.com/google/wire"
)

func initializeApp(cfg *config.Config) (*App, error) {
	wire.Build(
		server.NewServer,
		handler.NewHelloHandler,
		router.NewRouter,
		wire.Struct(new(App), "*"),
	)
	return nil, nil
}