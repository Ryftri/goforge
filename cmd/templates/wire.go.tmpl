//go:build wireinject
// +build wireinject

package main

import (
	"{{ .ModuleName }}/api/v1"
	"{{ .ModuleName }}/api/v1/handler"
	"github.com/gin-gonic/gin"
	"github.com/google/wire"
)

type App struct {
	Engine *gin.Engine
}

func initializeApp() *App {
	wire.Build(
		handler.NewHelloHandler,
		gin.Default,
		v1.SetupRouter,
		wire.Struct(new(App), "Engine"),
	)
	return nil
}